<mapper namespace="com.muzi.database02.system.webmagic.mapper.TitleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.muzi.database02.system.webmagic.entity.Title">
        <id column="id" property="id" />
